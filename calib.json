[
  {
    "output_id": "original",
    "filter": "PassthroughFilter"
  },
  {
    "filter": "FillRectFilter",
    "enabled": true,
    "params": {
      "top": 0,
      "bottom": 50
    }
  },
  {
    "filter": "GrayscaleFilter",
    "enabled": true
  },
  {
    "filter": "CannyFilter",
    "enabled": false,
    "params": {
      "thresh1": 30,
      "thresh2": 100,
      "aperture_size": 3
    }
  },
  {
    "filter": "HoughLinesPFilter",
    "enabled": false,
    "params": {
      "threshold": 45,
      "min_line_length": 80,
      "max_gap": 120
    }
  },
  {
    "output_id": "vanishing point",
    "filter": "VanishingPointFilter",
    "enabled": true,
    "params": {
      "threshold": 45,
      "min_line_length": 80,
      "max_gap": 120,
      "min_slope": 0.15
    }
  },
  {
    "name": "VP overlay",
    "filter": "MaskFilter",
    "enabled": true,
    "inputs": [
      "original",
      "vanishing point"
    ]
  }
]

