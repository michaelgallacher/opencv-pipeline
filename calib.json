[
  {
    "output_id": "original",
    "filter": "PassthroughFilter"
  },
  {
    "filter": "FillRectFilter",
    "enabled": true,
    "params": {
      "top": 0,
      "bottom": 50
    }
  },
  {
    "filter": "GrayscaleFilter",
    "enabled": true
  },
  {
    "filter": "CannyFilter",
    "enabled": false,
    "params": {
      "thresh1": 30,
      "thresh2": 100,
      "aperture_size": 3
    }
  },
  {
    "filter": "HoughLinesPFilter",
